class Main {
    static num fib(num n) {
        if (n < 2) return n;
        return fib(n - 1) + fib(n - 2);
    }

    static kaam main() {
        num total = 0;
        // Small upper bound to keep runtime reasonable in CI
        for (num i = 0; i < 20; i = i + 1) {
            total = total + fib(i);
        }
        printLine("fib-sum=" + total);
    }
}
