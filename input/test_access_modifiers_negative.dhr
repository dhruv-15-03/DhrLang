class Base {
	private num secret;
	protected num semi;
	public num open;
	kaam init(){ this.secret = 7; this.semi = 3; this.open = 1; }
	private num hidden(){ return 42; }
}
class Other {
	static kaam main(){ Base b = new Base();
		// Illegal direct private field access
		printLine(b.secret); // expect compile error
		// Illegal private method call
