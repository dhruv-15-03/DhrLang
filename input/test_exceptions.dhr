// Test Case: Exception Handling and Error Management
class ExceptionTest {
    static duo divide(num a, num b) {
        if (b == 0) {
            throw "Division by zero error";
        }
        return a / b;
    }
    
    static kaam riskyOperation() {
        throw "Something went wrong";
    }
    
    static kaam main() {
        // Basic try-catch
        try {
            duo result = ExceptionTest.divide(10, 2);
            printLine("Result: " + result);
        }
        catch(error) {
            printLine("Caught error: " + error);
        }
        
        // Try-catch with division by zero
        try {
            duo badResult = ExceptionTest.divide(10, 0);
            printLine("This should not print");
        }
        catch(error) {
            printLine("Caught division error: " + error);
        }
        finally {
            printLine("Finally block executed");
        }
        
        // Nested try-catch
        try {
            printLine("Outer try block");
            try {
                ExceptionTest.riskyOperation();
            }
            catch(innerError) {
                printLine("Inner catch: " + innerError);
                throw "Re-throwing from inner catch";
            }
        }
        catch(outerError) {
            printLine("Outer catch: " + outerError);
        }
        
        // Exception in loop
        for (num i = 2; i >= -1; i--) {
            try {
                duo result = ExceptionTest.divide(10, i);
                printLine("10 / " + i + " = " + result);
            }
            catch(error) {
                printLine("Error in iteration " + i + ": " + error);
            }
        }
        
        printLine("Program completed successfully");
    }
}
