// Test Case: Array Operations and Data Structures
class ArrayTest {
    static kaam main() {
        // Array creation and initialization
        num[] numbers = [1, 2, 3, 4, 5];
        sab[] names = ["Alice", "Bob", "Charlie"];
        
        // Array access and modification
        printLine("First number: " + numbers[0]);
        printLine("Last name: " + names[arrayLength(names) - 1]);
        
        numbers[2] = 99;
        printLine("Modified third number: " + numbers[2]);
        
        // Array iteration
        printLine("All numbers:");
        for (num i = 0; i < arrayLength(numbers); i++) {
            printLine("Index " + i + ": " + numbers[i]);
        }
        
        printLine("All names:");
        for (num j = 0; j < arrayLength(names); j++) {
            printLine("Name " + j + ": " + names[j]);
        }
        
        // Dynamic array creation
        num[] dynamicArray = new num[3];
        dynamicArray[0] = 10;
        dynamicArray[1] = 20;
        dynamicArray[2] = 30;
        
        printLine("Dynamic array:");
        for (num k = 0; k < arrayLength(dynamicArray); k++) {
            printLine("Value " + k + ": " + dynamicArray[k]);
        }
        
        // Array sum calculation
        num total = 0;
        for (num i = 0; i < arrayLength(numbers); i++) {
            total = total + numbers[i];
        }
        printLine("Sum of all numbers: " + total);
    }
}
