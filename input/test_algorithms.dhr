// Test Case: Simple Algorithms
class MathUtils {
    static num factorial(num n) {
        if (n <= 1) {
            return 1;
        }
        return n * MathUtils.factorial(n - 1);
    }
    
    static num gcd(num a, num b) {
        while (b != 0) {
            num temp = b;
            b = a % b;
            a = temp;
        }
        return a;
    }
    
    static kya isPrime(num n) {
        if (n <= 1) {
            return false;
        }
        for (num i = 2; i * i <= n; i++) {
            if (n % i == 0) {
                return false;
            }
        }
        return true;
    }
}

class AlgorithmTest {
    static kaam main() {
        // Test factorial
        for (num i = 1; i <= 6; i++) {
            printLine("Factorial of " + i + " = " + MathUtils.factorial(i));
        }
        
        // Test GCD
        printLine("GCD of 48 and 18: " + MathUtils.gcd(48, 18));
        printLine("GCD of 100 and 25: " + MathUtils.gcd(100, 25));
        
        // Test prime checking
        printLine("Prime numbers from 1 to 20:");
        for (num i = 1; i <= 20; i++) {
            if (MathUtils.isPrime(i)) {
                print(i + " ");
            }
        }
        printLine("");
        
        printLine("Algorithm tests completed successfully!");
    }
}
